<p id="notice"><%= notice %></p>

<h1>Plans</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Robotidd</th>
      <th>Projectidd</th>
      <th>Instruments</th>
      <th>Location</th>
      <th>Procedure</th>
      <th>Objective</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @planns.each do |plann| %>
      <tr>
        <td><%= plann.title %></td>
        <td><%= plann.robotidd %></td>
        <td><%= plann.projectidd %></td>
        <td><%= plann.instruments %></td>
        <td><%= plann.location %></td>
        <td><%= plann.procedure %></td>
        <td><%= plann.objective %></td>
        <td><%= plann.comments %></td>
        <td><%= link_to 'Show', plann %></td>
        <td><%= link_to 'Edit', edit_plann_path(plann) %></td>
        <td><%= link_to 'Destroy', plann, method: :delete, data: { confirm: 'Are you sure?' } %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plan', new_plann_path %>
