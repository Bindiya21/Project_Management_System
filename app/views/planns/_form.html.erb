<%= form_with(model: plann, local: true) do |form| %>
  <% if plann.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(plann.errors.count, "error") %> prohibited this plann from being saved:</h2>

      <ul>
        <% plann.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="field">
    <%= form.label :robotidd %>
 <%= form.collection_select(:robotidd, Robot.all, :robotidd,:title, { :prompt => 'Select a Robot', :selected => @robotidd}, { class: 'form-control' }) %>
  </div>

  <div class="field">
    <%= form.label :projectidd %>
    <%= form.collection_select(:projectidd, Project.all, :projectidd,:title, { :prompt => 'Select a Project', :selected => @projectidd}, { class: 'form-control' }) %>
  </div>

  <div class="field">
    <%= form.label :instruments %>
    <%= form.text_area :instruments %>
  </div>

  <div class="field">
    <%= form.label :location %>
    <%= form.text_area :location %>
  </div>

  <div class="field">
    <%= form.label :procedure %>
    <%= form.text_area :procedure %>
  </div>

  <div class="field">
    <%= form.label :objective %>
    <%= form.text_area :objective %>
  </div>

  <div class="field">
    <%= form.label :comments %>
    <%= form.text_area :comments %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
